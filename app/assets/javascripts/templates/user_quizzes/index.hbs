<h2>My Quizzes</h2>

<table>
  <tr>
    <th>title</th>
    <th>progress</th>
  </tr>

  {{#each quizzes}}
  <tr class="start-quiz" data-user-quiz={{id}}>
    <td>{{title}}</td>
    <td class="progress">{{progress}}</td>
  </tr> 
  <button class="quiz-overview" data-user-quiz={{id}}>Overview</button>
  {{/each}}
</table>

